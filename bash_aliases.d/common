alias repos-update='sudo dnf check-update || true && sudo dnf update -y && flatpak update -y && sudo rclone selfupdate && pip install --upgrade system-monitoring-center'
alias repos-clean='sudo dnf autoremove -y && flatpak uninstall --unused'
alias la='ls -la'

alias fli='flatpak install'
alias fll='flatpak list --app'
alias fls='flatpak search'

alias dnfi='sudo dnf install'
alias dnfli='dnf list --installed'
alias dnfs='dnf search'
