alias repos-update='
sudo dnf check-update || true
sudo dnf update -y
flatpak update -y
sudo rclone selfupdate
sdk update
wget https://github.com/mikefarah/yq/releases/latest/download/yq_linux_amd64 -O /usr/bin/yq
chmod +x /usr/bin/yq'

alias repos-clean='
sudo dnf autoremove -y
flatpak uninstall --unused'

alias la='ls -la'

alias fli='flatpak install'
alias fll='flatpak list --app'
alias fls='flatpak search'

alias dnfi='sudo dnf install'
alias dnfli='dnf list --installed'
alias dnfs='dnf search'

alias usejava8='sdk use java 8.0.302-open'
alias usejava11='sdk use java 11.0.12-open'
alias usejava17='sdk use java 17.0.2-open'
