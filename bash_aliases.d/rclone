# google_drive_private: aliases
alias mount-read-gdp='rclone mount google_drive_private: ${HOME}/cloud_mounts/google_drive_private/ --no-modtime --read-only --vfs-cache-mode full --daemon'
alias mount-full-gdp='rclone mount google_drive_private: ${HOME}/cloud_mounts/google_drive_private/ --no-modtime --vfs-cache-mode full --daemon'
alias unmount-gdp='fusermount -u ${HOME}/cloud_mounts/google_drive_private'
alias serve-gdp='rclone serve http google_drive_private: --no-modtime --read-only --vfs-cache-mode full'
